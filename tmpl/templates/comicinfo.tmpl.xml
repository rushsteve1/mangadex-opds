<?xml version="1.0" encoding="UTF-8" ?>
<ComicInfo>
    <Title>{{ html .FullTitle }}</Title>
    <Series>{{ html .Manga.TrTitle }}</Series>
    <Number>{{ html .Attributes.Chapter }}</Number>
