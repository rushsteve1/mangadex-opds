    <PageCount>{{ html .Attributes.Pages }}</PageCount>
    <Genre>{{ html .Manga.Attributes.Demographic }}</Genre>
    <LanguageISO>{{ html .Attributes.TranslatedLanguage }}</LanguageISO>
    <Manga>YesAndRightToLeft</Manga>
</ComicInfo>
